import java.util.*;

public class Solution {
	public int[] solution(int []arr) {
        int[] answer = {};
        
        for(int i=0;i<arr.length-1;i++) {
            if(arr[i]==arr[i+1]) {
                for(int j=i;j<arr.length;j++) {
                    arr[j+1]=arr[j+2];
                }
            }
            else continue;
        }
        
        for(int i=0;i<arr.length;i++) {
            answer[i] = arr[i];
        }
        
        return answer;
	}
}
