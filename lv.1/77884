class Solution {
    int countDivisor(int num) {
        int cntDiv = 0;
        for(int i=1;i<=num;i++) {
            if(num%i==0) ++cntDiv;
        }
        return cntDiv;
    }
    
    public int solution(int left, int right) {
        int answer = 0;
        
        for(int i=left;i<=right;i++) {
            if(countDivisor(i)%2==0) answer += i;
            if(countDivisor(i)%2==1) answer -= i;
        }
        
        return answer;
    }
}
